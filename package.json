{
  "name": "mitsumori-server",
  "version": "1.0.0",
  "main": "../build/server.js",
  "repository": "https://github.com/UNRULYEON/mitsumori",
  "author": "Amar Kisoensingh <amar@kisoensingh.sh>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start:prod": "NODE_ENV=production node build/server.js",
    "start:dev": "NODE_ENV=development nodemon server.ts",
    "build:prod": "npx tsc && mkdir build/client && cd client && yarn && yarn run build:prod && cd ../ && mv ./client/build/* ./build/client",
    "build:dev": "npx tsc"
  },
  "dependencies": {
    "express": "^4.17.1",
    "nedb": "^1.8.0",
    "socket.io": "^3.0.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/nedb": "^1.8.11",
    "@types/socket.io": "^2.1.12",
    "nodemon": "^2.0.6"
  }
}
